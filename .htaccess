



RewriteEngine on



RewriteRule ^([a-zA-Z-0-9_]+)/([a-zA-Z-0-9_]+)/([a-zA-Z-0-9_]+)/(en|pl)$ index.php?module=$1&type=$2&more=$3&lang=$4 [L]

RewriteRule ^([a-zA-Z-0-9_]+)/([a-zA-Z-0-9_]+)/(en|pl)$ index.php?module=$1&type=$2&lang=$3 [L]

RewriteRule ^([a-zA-Z-0-9_]+)/(en|pl)$ index.php?module=$1&lang=$2 [L]



RewriteRule ^([a-zA-Z-0-9_]+)/([a-zA-Z-0-9_]+)/([a-zA-Z-0-9_]+)/([a-zA-Z-0-9_]+)$ index.php?module=$1&type=$2&more=$3&bId=$4 [L]

RewriteRule ^([a-zA-Z-0-9_]+)/([a-zA-Z-0-9_]+)/([a-zA-Z-0-9_]+)$ index.php?module=$1&type=$2&more=$3 [L]

RewriteRule ^([a-zA-Z-0-9_]+)/([a-zA-Z-0-9_]+)$ index.php?module=$1&type=$2 [L]

RewriteRule ^([a-zA-Z-0-9_]+)$ index.php?module=$1 [L]
